REM [000][TOKTG_KHN]
@ECHO OFF

REM ログフォルダ作成
IF NOT EXIST %~DP0..\logs MKDIR %~DP0..\logs

REM コマンドフォルダ作成
IF NOT EXIST %~DP0..\cmd MKDIR %~DP0..\cmd

REM ログファイルの情報
SET OUTFILE=%~DP0../logs/setup.log
SET OUTTIME=%~DP0../logs/time.log

REM ベースフォルダ
SET BASEDIR=%~DP0..

REM 更新対象の情報
SET INFOSCRIPT=%~DP0..\cmd\%~N0.DB2

SET MSG=%DATE% %TIME% INATK2.TOKTG_KHN アップデート処理（全店特売（アンケート有）_基本）
ECHO %MSG%
>> %OUTFILE% ECHO %MSG%

"%MYSQLPATH%mysql" %OPTION% -h %HOST% %DB_NAME% -u %USER_ID% -N -e "UPDATE INATK2.TOKTG_KHN KHN SET JLSTCREFLG = 1 WHERE EXISTS (SELECT 1 FROM (SELECT MOYSKBN, MOYSSTDT, MOYSRBAN FROM INAWK.JH_LIST_DIFF) JH WHERE JH.MOYSKBN = KHN.MOYSKBN AND JH.MOYSSTDT = KHN.MOYSSTDT AND JH.MOYSRBAN = KHN.MOYSRBAN);COMMIT;" >>%OUTFILE% 2>&1
IF %ERRORLEVEL% GTR 1 GOTO ERROR
"%MYSQLPATH%mysql" %OPTION% -h %HOST% %DB_NAME% -u %USER_ID% -N -e "ANALYZE TABLE INATK2.TOKTG_KHN;" >>%OUTFILE% 2>&1
IF NOT %ERRORLEVEL%==0 GOTO ERROR


:FINAL
EXIT /B 0

:ERROR
EXIT /B 1
