REM [026][069_LOAD]
@ECHO OFF

REM ログフォルダ作成
IF NOT EXIST %~DP0..\logs MKDIR %~DP0..\logs

REM コマンドフォルダ作成
IF NOT EXIST %~DP0..\cmd MKDIR %~DP0..\cmd

REM ログファイルの情報
SET OUTFILE=%~DP0../logs/setup.log
SET OUTTIME=%~DP0../logs/time.log

REM ベースフォルダ
SET BASEDIR=%~DP0..

REM 更新対象の情報
SET INFOSCRIPT=%~DP0..\cmd\%~N0.DB2


SET INFILENAME=
SET MSG=%DATE% %TIME% 次週定量_店別数量 INATK.HATJTR_TEN BACKUP処理
SET OUTFILENAME=%~DP0..\DATA\INATK.HATJTR_TEN
ECHO %OUTFILENAME% >> %OUTFILE%
ECHO %MSG%
>> %OUTFILE% ECHO %MSG%
SET OUTFILENAME=%~DP0..\DATA\INATK.HATJTR_TEN
"%MYSQLPATH%mysql" %OPTION% -h %HOST% %DB_NAME% -u %USER_ID% -N -e "SELECT RPAD(SHNCD, 14, ' ') || ',' || COALESCE(BINKBN, '') || ',' || COALESCE(TENCD, '') || ',' || COALESCE(SHUNO, '') || ',' || COALESCE(SURYO_MON, '') || ',' || COALESCE(SURYO_TUE, '') || ',' || COALESCE(SURYO_WED, '') || ',' || COALESCE(SURYO_THU, '') || ',' || COALESCE(SURYO_FRI, '') || ',' || COALESCE(SURYO_SAT, '') || ',' || COALESCE(SURYO_SUN, '') || ',' || COALESCE(SENDFLG, '') || ',' || COALESCE(OPERATOR, '') || ',' || COALESCE(ADDDT, '') || ',' || COALESCE(UPDDT, '') FROM INATK.HATJTR_TEN;" >%OUTFILENAME% 2>>%OUTFILE%

SET MSG=%DATE% %TIME% 次週定量_店別数量 INATK.HATJTR_TEN LOAD処理
ECHO %MSG%
>> %OUTFILE% ECHO %MSG%
"%MYSQLPATH%mysql" %OPTION% -h %HOST% %DB_NAME% -u %USER_ID% --show-warnings -vv -e "TRUNCATE TABLE INATK.HATJTR_TEN; INSERT IGNORE INTO INATK.HATJTR_TEN (SELECT SHNCD, BINKBN, TENCD, SHUNO, SURYO_MON, SURYO_TUE, SURYO_WED, SURYO_THU, SURYO_FRI, SURYO_SAT, SURYO_SUN, 1, OPERATOR, ADDDT, UPDDT FROM INATK2.HATJTR_TEN); commit;" >>%OUTFILE% 2>&1
IF NOT %ERRORLEVEL%==0 GOTO ERROR
"%MYSQLPATH%mysql" %OPTION% -h %HOST% %DB_NAME% -u %USER_ID% --show-warnings -vv -e "ANALYZE TABLE INATK.HATJTR_TEN;" >>%OUTFILE% 2>&1
IF ERRORLEVEL 1 GOTO ERROR



:FINAL
EXIT /B 0

:ERROR
EXIT /B 1
