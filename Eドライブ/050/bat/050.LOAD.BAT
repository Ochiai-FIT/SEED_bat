REM [026][069_LOAD]
@ECHO OFF

REM ログフォルダ作成
IF NOT EXIST %~DP0..\logs MKDIR %~DP0..\logs

REM コマンドフォルダ作成
IF NOT EXIST %~DP0..\cmd MKDIR %~DP0..\cmd

REM ログファイルの情報
SET OUTFILE=%~DP0../logs/setup.log
SET OUTTIME=%~DP0../logs/time.log

REM ベースフォルダ
SET BASEDIR=%~DP0..

REM 更新対象の情報
SET INFOSCRIPT=%~DP0..\cmd\%~N0.DB2


SET INFILENAME=
SET MSG=%DATE% %TIME% 催し検索 INAIN.TOKMM LOAD処理
ECHO %MSG%
>> %OUTFILE% ECHO %MSG%
"%MYSQLPATH%mysql" %OPTION% -h %HOST% %DB_NAME% -u %USER_ID% --show-warnings -vv -e "TRUNCATE TABLE INATK.TOKMM; INSERT IGNORE INTO INATK.TOKMM (SELECT T1.MOYSKBN, T1.MOYSSTDT, T1.MOYSRBAN, 0 AS BMFLG, T1.HBSTDT, T1.HBEDDT, CASE WHEN T1.NNSTDT < T1.NNSTDT_TGF THEN T1.NNSTDT ELSE T1.NNSTDT_TGF END AS NNSTDT, CASE WHEN T1.NNEDDT > T1.NNEDDT_TGF THEN T1.NNEDDT ELSE T1.NNEDDT_TGF END AS NNEDDT, 'BATCH' AS OPERATOR, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP FROM INATK.TOKMOYCD T1 UNION ALL SELECT T1.MOYSKBN, T1.MOYSSTDT, T1.MOYSRBAN, 1 AS BMFLG, T1.HBSTDT, T1.HBEDDT, CASE WHEN T1.NNSTDT < T1.NNSTDT_TGF THEN T1.NNSTDT ELSE T1.NNSTDT_TGF END AS NNSTDT, CASE WHEN T1.NNEDDT > T1.NNEDDT_TGF THEN T1.NNEDDT ELSE T1.NNEDDT_TGF END AS NNEDDT, 'BATCH' AS OPERATOR, CURRENT_TIMESTAMP, CURRENT_TIMESTAMP FROM INATK.TOKMOYCD T1 INNER JOIN (SELECT DISTINCT MOYSKBN, MOYSSTDT, MOYSRBAN FROM INATK.TOKBM WHERE UPDKBN = 0) T2 ON T2.MOYSKBN = T1.MOYSKBN AND T2.MOYSSTDT = T1.MOYSSTDT AND T2.MOYSRBAN = T1.MOYSRBAN AND T1.UPDKBN = 0 ORDER BY 1, 2, 3, 4); commit;" >>%OUTFILE% 2>&1
IF NOT %ERRORLEVEL%==0 GOTO ERROR
"%MYSQLPATH%mysql" %OPTION% -h %HOST% %DB_NAME% -u %USER_ID% --show-warnings -vv -e "ANALYZE TABLE INATK.TOKMM;" >>%OUTFILE% 2>&1
IF ERRORLEVEL 1 GOTO ERROR


:FINAL
EXIT /B 0

:ERROR
EXIT /B 1
